def testehttp(request):
	#import pdb; pdb.set_trace()
	if request.method=="GET":
		print " >> > > >> leu a URL"
		print request.GET
	else:
		print " > > > > >  Enviou algo pra URL"
		print request.POST

	return render(request, 'curriculo/testehttp.html')
